{
  "exclude": [
    "node_modules"
  ],
  "extends": "@brent/tsconfig/next.json",
  "include": [
    "./",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/service/*": [
        "service/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/context/*": [
        "context/*"
      ],
      "@/metadata/*": [
        "metadata/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
